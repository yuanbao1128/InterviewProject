{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowJs": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": ".vercel/ts-out",
    "types": ["node"]
  },
  "include": ["**/*.ts","../_src/**/*.ts"],
  "exclude": ["dist", "node_modules", ".vercel", ".vercel/output"]
}